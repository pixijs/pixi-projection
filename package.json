{
	"name": "pixi-projection",
	"version": "1.0.0",
	"description": "Projections (2.5d, 3d and bilinear) for pixi v^7",
	"author": "Ivan Popelyshev",
	"contributors": [
		"Ivan Popelyshev <ivan.popelyshev@gmail.com>"
	],
	"main": "./lib/index.js",
	"module": "./lib/index.mjs",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"import": "./lib/index.mjs",
			"require": "./lib/index.js",
			"types": "./lib/index.d.ts"
		}
	},
	"extensionConfig": {
		"lint": [
			"src"
		],
		"namespace": "PIXI.projection",
		"docsName": "PixiJS 2d-3d projections",
		"docsCopyright": "Copyright &copy; 2015 - 2023 Ivan Popelyshev",
		"docsTitle": "PixiJS 2d-3d projection API Documentation",
		"docsDescription": "Documentation for PixiJS 2.5D projection library",
		"docsKeywords": "docs, documentation, pixi, pixijs, rendering, 3d, projection, javascript"
	},
	"homepage": "http://www.pixijs.com/",
	"bugs": "https://github.com/pixijs/projection/issues",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/pixijs/projection.git"
	},
	"scripts": {
		"clean": "xs clean",
		"start": "xs serve",
		"watch": "xs watch",
		"build": "xs build",
		"lint": "xs lint",
		"lint:fix": "xs lint --fix",
		"types": "xs types",
		"release": "xs release",
		"docs": "xs docs",
		"deploy": "xs deploy",
		"test": "xs build,docs"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=16",
		"npm": ">=8"
	},
	"files": [
		"dist/",
		"lib/",
		"global.d.ts"
	],
	"peerDependencies": {
		"@pixi-spine/base": "^4.0.3",
		"@pixi/core": "^7.2.0",
		"@pixi/display": "^7.2.0",
		"@pixi/graphics": "^7.2.0",
		"@pixi/mesh": "^7.2.0",
		"@pixi/mesh-extras": "^7.2.0",
		"@pixi/sprite": "^7.2.0",
		"@pixi/sprite-tiling": "^7.2.0",
		"@pixi/text": "^7.2.0"
	},
	"devDependencies": {
		"@pixi/core": "^7.2.0",
		"@pixi/display": "^7.2.0",
		"@pixi/extension-scripts": "^1.8.1",
		"@pixi/graphics": "^7.2.0"
	}
}
